   <mif:entryPoint graphicLinkId="06231303-DE2E-B41F-859A-A5F95612E2CE" id="COCT_MT570000UV"
                   name="A_LaboratoryProcessStep universal"
                   className="LabProcessStep">
      <mif:annotations>
         <mif:documentation>
            <mif:description>
               <mif:text>Description: Used to communicate where the Lab is in the process of producing a test result.</mif:text>
            </mif:description>
         </mif:documentation>
      </mif:annotations>
   </mif:entryPoint>
   <mif:containedClass>
      <mif:class graphicLinkId="1D2796CE-FED1-E402-3E7E-6C8972B71BD2" name="Collector"
                 isAbstract="false">
         <mif:derivedFrom staticModelDerivationId="1" className="Role"/>
         <mif:derivedFrom staticModelDerivationId="2" className="Collector"/>
         <mif:derivedFrom staticModelDerivationId="3" className="Collector"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <mif:p>The Collector Role (as an assigned entity) is used to document at a high level what role is responsible for collecting a specimen. This class carries some of the functionality found in the 2.x Specimen Action Code (OBR.11).</mif:p>
                  </mif:text>
               </mif:definition>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="ASSIGNED"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Role" attributeName="classCode"/>
            <mif:derivedFrom staticModelDerivationId="2" className="Collector" attributeName="classCode"/>
            <mif:derivedFrom staticModelDerivationId="3" className="Collector" attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
               <mif:code codeSystemName="RoleClass" code="ASSIGNED"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="code" sortKey="2" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The mandatory code attribute is used to identify at a high level who is repsonsible for collecting a specimen. Types of values include "laboratory", "phlebotomist", "pathologist", etc.</mif:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Role" attributeName="code"/>
            <mif:derivedFrom staticModelDerivationId="2" className="Collector" attributeName="code"/>
            <mif:derivedFrom staticModelDerivationId="3" className="Collector" attributeName="code"/>
            <mif:type name="CE"/>
            <mif:vocabulary>
               <mif:conceptDomain name="LabSpecimenCollectionProviders"/>
            </mif:vocabulary>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
