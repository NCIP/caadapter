         <mif:attribute name="effectiveTime" sortKey="7" minimumMultiplicity="0"
                        maximumMultiplicity="1"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The optional effectiveTime attribute is used to indicate the time (or interval of time) that the process step was active.</mif:p>
                     </mif:text>
                  </mif:definition>
