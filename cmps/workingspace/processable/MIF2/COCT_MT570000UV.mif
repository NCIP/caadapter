            <mif:derivedFrom staticModelDerivationId="2" className="LabProcessStep" attributeName="moodCode"/>
            <mif:derivedFrom staticModelDerivationId="3" className="LabProcessStep" attributeName="moodCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary>
