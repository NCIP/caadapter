                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The optional text attribute is used to describe the process step.</mif:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="text"/>
            <mif:derivedFrom staticModelDerivationId="2" className="LabProcessStep" attributeName="text"/>
            <mif:derivedFrom staticModelDerivationId="3" className="LabProcessStep" attributeName="text"/>
