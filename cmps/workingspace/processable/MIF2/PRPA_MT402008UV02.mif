   <mif:entryPoint name="EncounterEvent" useKind="SemanticPayload" className="EncounterEvent">
      <mif:annotations>
         <mif:documentation>
            <mif:description>
               <mif:text>
                  <mif:p>This R-MIM defines the message used to report that inpatient encounters for two different patients are linked (or unlinked) for care and billing purposes.</mif:p>
               </mif:text>
            </mif:description>
         </mif:documentation>
      </mif:annotations>
   </mif:entryPoint>
   <mif:containedClass>
