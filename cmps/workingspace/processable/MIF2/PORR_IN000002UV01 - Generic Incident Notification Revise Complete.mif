<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.5" name="000002"
                 title="Generic Incident Notification Event Revise Notification">
   <mif:businessName name="Generic Incident Notification Revise Complete"/>
   <mif:packageLocation root="DEFN" section="HM" subSection="PO" domain="RR" artifact="IN"
                        realmNamespace="UV"
                        version="01"
                        id="000002"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>This is the message interaction used to send a revised completed Patient Safety Generic Incident Report to update information that has previously been sent from the specific healthcare facility/site to an agency that collects Patient Safety incident information.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="HM" subSection="PO" domain="RR" artifact="TE"
                             realmNamespace="UV"
                             version="01"
                             id="150002"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT"
                        realmNamespace="UV"
                        version="01"
                        id="000100">
      <mif:parameterModel parameterName="ControlActProcess" root="DEFN" section="IM" subSection="MC"
                          domain="AI"
                          artifact="MT"
                          realmNamespace="UV"
                          version="01"
                          id="700201"
                          traversalName="controlActProcess">
         <mif:parameterModel parameterName="Act" root="DEFN" section="HM" subSection="PO" domain="RR"
                             artifact="MT"
                             realmNamespace="UV"
                             version="01"
                             id="150101"
                             traversalName="investigationEvent"/>
      </mif:parameterModel>
   </mif:argumentMessage>
   <mif:receiverResponsibilities>
      <mif:reason>
         <mif:text>  
                  <mif:p>No formal receiver responsibilities are described except the sending of a system level acknowledgement of receipt; but there is an implicit understanding that the receiving system will update as a result of this new information received</mif:p>  
               </mif:text>
      </mif:reason>
      <mif:invokeInteraction root="DEFN" section="HM" subSection="PO" domain="RR" artifact="IN"
                             realmNamespace="UV"
                             version="01"
                             id="000002"/>
      <mif:invokeTriggerEvent root="DEFN" section="HM" subSection="PO" domain="RR" artifact="TE"
                              realmNamespace="UV"
                              version="01"
                              id="150002"/>
   </mif:receiverResponsibilities>
</mif:interaction>