            <mif:derivedFrom staticModelDerivationId="2" className="Approval" attributeName="id"/>
            <mif:type name="II"/>
         </mif:attribute>
         <mif:attribute name="code" sortKey="4" minimumMultiplicity="0" maximumMultiplicity="1"
                        isMandatory="false"
                        conformance="R"
                        isImmutable="false">
            <mif:annotations/>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="code"/>
            <mif:derivedFrom staticModelDerivationId="2" className="Approval" attributeName="code"/>
            <mif:type name="CE"/>
            <mif:vocabulary>
               <mif:conceptDomain name="ActMedicineApproval"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="statusCode" sortKey="5" minimumMultiplicity="0" maximumMultiplicity="1"
                        isMandatory="false"
                        conformance="R"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:description>
                     <mif:p>Tells whether approval was granted (complete) or whether the approval is presently in progress (active). Thus, a substance or product that is not approved yet but about which an approval is in process and approval number is known can also have the Approval structure but must indicate a status of "active" in the Approval.statusCode. Likewise, a substance or product whose approval has been withdrawn would have a statusCode of "obsolete".</mif:p>
                  </mif:description>
                  <mif:designComments>
                     <mif:text>
                        <mif:p>active: not yet approved; complete: approved; obsolete: withdrawn</mif:p>
                     </mif:text>
                  </mif:designComments>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Act" attributeName="statusCode"/>
            <mif:derivedFrom staticModelDerivationId="2" className="Approval" attributeName="statusCode"/>
            <mif:type name="CS"/>
