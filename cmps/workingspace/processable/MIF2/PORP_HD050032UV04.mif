                        isMandatory="false"
                        conformance="R"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:description>
                     <mif:p>A ratio specifying the strength of the ingredient as amount of ingredient (numerator) in amount of medicine (denominator).</mif:p>
                  </mif:description>
                  <mif:designComments>
                     <mif:text>
                        <mif:p>Strength</mif:p>
