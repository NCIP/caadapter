         <mif:attribute name="id" sortKey="3" minimumMultiplicity="1" maximumMultiplicity="*"
                        conformance="R"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <mif:p>The PolicyOrProgram id: Zero or more unique identifiers for this policy or program must be sent if available. Note that for the A_Coverage identified and identified-confirmable variants, the lower cardinality is one.</mif:p>
                     </mif:text>
