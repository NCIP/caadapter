<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.5" name="801010"
                 title="Rtls Event Notification Observe Co-located Patient Provider">
   <mif:businessName name="Patient and Provider Co-located"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="PR" domain="RG" artifact="IN"
                        realmNamespace="UV"
                        version="01"
                        id="801010"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>This notification is sent from a real time location tracking system to convey that a patient and a provider are now in the same location. Locally defined business rules determine which locations are of interest and how long the resources need to be in that location to trigger this interaction. PositionObservationEvent two subject Person HealthCareProvider LivingSubjectChoice Person NonPersonLivingSubject Patient</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
