<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.5" name="403002"
                 title="Emergency Encounter Event Revise Notification">
   <mif:businessName name="Emergency Encounter Revised"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="PR" domain="PA" artifact="IN"
                        realmNamespace="UV"
                        version="02"
                        id="403002"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>This interaction occurs after information about an emergency encounter changes (except for changes to Attending Practitioner, Patient Location and Responsible Organization participations that cause separate interactions). An informer sends to a tracker a complete emergency encounter record, including related acts and participations.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
