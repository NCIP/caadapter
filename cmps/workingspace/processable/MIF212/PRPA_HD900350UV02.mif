                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">An interval of time specifying the period this healthcare practitioner was actively involved in authorizing this patient encounter</html:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Participation" attributeName="time"/>
