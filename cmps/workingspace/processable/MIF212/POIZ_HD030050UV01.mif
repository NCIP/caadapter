                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">A coded value denoting the body area where the immunization was administered.</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Site of administration is required for follow up in case of an adverse event and in some jurisdictions is part of the minimum data required per national standard and legal client record.</html:p>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Attribute is marked "optional" to allow for use of pre-coordinated SNOMED Codes.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
               <mif:appInfo>
                  <mif:mapping sourceName="Unspecified">
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">PHSS.6800.Site PHSS.6700.Site</html:p>
                     </mif:text>
                  </mif:mapping>
               </mif:appInfo>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="SubstanceAdministration"
                             attributeName="approachSiteCode"/>
            <mif:derivedFrom staticModelDerivationId="2" className="Immunization"
                             attributeName="approachSiteCode"/>
            <mif:type name="CV"/>
            <mif:vocabulary codingStrength="CNE">
               <mif:vocabularyDomain name="HumanSubstanceAdministrationSite"/>
            </mif:vocabulary>
         </mif:attribute>
