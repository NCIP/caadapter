                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The route by which the drug was administered to the patient, for example, sub-cutaneous, intra-muscular</html:p>
                     </mif:text>
                  </mif:definition>
                  <mif:rationale>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Ensures consistency in description of routes.</html:p>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">Attribute is marked 'optional' to allow for use of pre-corordinated SNOMED Codes.</html:p>
                     </mif:text>
                  </mif:rationale>
               </mif:documentation>
               <mif:appInfo>
                  <mif:mapping sourceName="Unspecified">
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">PHSS.6800.Route of administration PHSS.6700.Route of administration</html:p>
                     </mif:text>
                  </mif:mapping>
               </mif:appInfo>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="SubstanceAdministration"
                             attributeName="routeCode"/>
            <mif:derivedFrom staticModelDerivationId="2" className="Immunization" attributeName="routeCode"/>
