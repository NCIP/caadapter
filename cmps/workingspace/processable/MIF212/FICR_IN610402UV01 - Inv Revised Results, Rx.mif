<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.2" name="610402"
                 title="Invoice Event Revise Notification Tracker Pharmacy">
   <mif:businessName name="Inv Revised Results, Rx"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="FI" domain="CR" artifact="IN"
                        realmNamespace="UV"
                        version="01"
                        id="610402"/>
   <mif:annotations>
      <mif:documentation>
