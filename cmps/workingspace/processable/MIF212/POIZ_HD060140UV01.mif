   <mif:entryPoint graphicLinkId="34698EBB-1191-4A6E-0C2C-7BE306227641" id="POIZ_HD060140UV01"
                   name="Immunization query"
                   className="ParameterList">
      <mif:annotations>
         <mif:documentation>
            <mif:description>
               <mif:text>Entry point for Immunization Query message. The intent of this query is to find detailed information about an identified immunization event. Subject information is included to corroborate the immunization event id. </mif:text>
            </mif:description>
         </mif:documentation>
      </mif:annotations>
   </mif:entryPoint>
   <mif:containedClass>
