<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.2" name="010070"
                 title="Immunization Event Revise Request Immunization">
   <mif:businessName name="Update immunization request"/>
   <mif:packageLocation root="DEFN" section="HM" subSection="PO" domain="IZ" artifact="IN"
                        realmNamespace="UV"
                        version="01"
                        id="010070"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>Request that information be added to a previously recorded immunization.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="HM" subSection="PO" domain="IZ" artifact="TE"
                             realmNamespace="UV"
                             version="01"
                             id="010090"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT"
                        realmNamespace="UV"
                        version="01"
                        id="000100">
      <mif:parameterModel parameterName="ControlActProcess" root="DEFN" section="IM" subSection="MC"
                          domain="AI"
                          artifact="MT"
                          realmNamespace="UV"
                          version="01"
                          id="700201"
                          traversalName="controlActProcess">
         <mif:parameterModel parameterName="Act" root="DEFN" section="HM" subSection="PO" domain="IZ"
                             artifact="MT"
                             realmNamespace="UV"
                             version="01"
                             id="030060"
                             traversalName="immunization"/>
      </mif:parameterModel>
   </mif:argumentMessage>
   <mif:receiverResponsibilities>
      <mif:reason>
         <html:text xmlns:html="http://www.w3.org/1999/xhtml">  
                  <html:p>Indicates that the revision has been accepted.</html:p>  
               </html:text>
      </mif:reason>
      <mif:invokeInteraction root="DEFN" section="HM" subSection="PO" domain="IZ" artifact="IN"
                             realmNamespace="UV"
                             version="01"
                             id="010080"/>
      <mif:invokeTriggerEvent root="DEFN" section="HM" subSection="PO" domain="IZ" artifact="TE"
                              realmNamespace="UV"
                              version="01"
                              id="010050"/>
   </mif:receiverResponsibilities>
   <mif:receiverResponsibilities>
      <mif:reason>
         <html:text xmlns:html="http://www.w3.org/1999/xhtml">  
                  <html:p>Indicates that the revision has been rejected.</html:p>  
               </html:text>
      </mif:reason>
      <mif:invokeInteraction root="DEFN" section="HM" subSection="PO" domain="IZ" artifact="IN"
                             realmNamespace="UV"
                             version="01"
                             id="010090"/>
      <mif:invokeTriggerEvent root="DEFN" section="HM" subSection="PO" domain="IZ" artifact="TE"
                              realmNamespace="UV"
                              version="01"
                              id="010020"/>
   </mif:receiverResponsibilities>
</mif:interaction>