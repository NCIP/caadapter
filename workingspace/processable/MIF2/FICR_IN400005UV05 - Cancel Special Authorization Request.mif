<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.5" name="400005"
                 title="Special Authorization Request Abort Request">
   <mif:businessName name="Cancel Special Authorization Request"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="FI" domain="CR" artifact="IN"
                        realmNamespace="UV"
                        version="05"
                        id="400005"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>The Cancel Special Authorization Request is submitted to cancel processing of a previously submitted Special Authorization Request. This interaction is typically submitted when a provider determines that the requirement for an SA Request is no longer appropriate.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="AM" subSection="FI" domain="CR" artifact="TE"
                             realmNamespace="UV"
                             version="05"
                             id="400005"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="AI" artifact="MT"
                        realmNamespace="UV"
                        version="01"
                        id="700201">
      <mif:parameterModel parameterName="Act" root="DEFN" section="IM" subSection="CO" domain="MT"
                          artifact="MT"
                          realmNamespace="UV"
                          version="01"
                          id="001103"
                          traversalName="actGenericStatus"/>
   </mif:argumentMessage>
   <mif:receiverResponsibilities>
      <mif:reason>
         <mif:text>  
                  <mif:p>The receiving system confirms that the Special Authorization request has been cancelled.</mif:p>  
               </mif:text>
      </mif:reason>
      <mif:invokeInteraction root="DEFN" section="AM" subSection="FI" domain="CR" artifact="IN"
                             realmNamespace="UV"
                             version="05"
                             id="400006"/>
      <mif:invokeTriggerEvent root="DEFN" section="AM" subSection="FI" domain="CR" artifact="TE"
                              realmNamespace="UV"
                              version="05"
                              id="400006"/>
   </mif:receiverResponsibilities>
   <mif:receiverResponsibilities>
      <mif:reason>
         <mif:text>  
                  <mif:p>The receiving system is unable to cancel the Special Authorization request.</mif:p>  
               </mif:text>
      </mif:reason>
      <mif:invokeInteraction root="DEFN" section="AM" subSection="FI" domain="CR" artifact="IN"
                             realmNamespace="UV"
                             version="05"
                             id="400007"/>
      <mif:invokeTriggerEvent root="DEFN" section="AM" subSection="FI" domain="CR" artifact="TE"
                              realmNamespace="UV"
                              version="05"
                              id="400007"/>
   </mif:receiverResponsibilities>
</mif:interaction>