                 title="Invoice Event Activate Request Complete Vision Care">
   <mif:businessName name="Inv Request, VisionCare"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="FI" domain="CR" artifact="IN"
                        realmNamespace="UV"
                        version="03"
                        id="600106"/>
