<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.5" name="520107"
                 title="Pre-Determination Event Nullify Request Complete Physician">
   <mif:businessName name="Pre-Det Nullify Request, Physician"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="FI" domain="CR" artifact="IN"
                        realmNamespace="UV"
                        version="03"
                        id="520107"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>The sender sends a Pre-Determination Nullify Request message to request the nullification of a previously submitted Pre-Determination Request for medical services and/or products rendered by a physician. The receiver will provide completed nullify results only. The receiver does NOT return an initial response. Nullify results indicate that the receiver was successful or not in nullifying the Pre-Determination Request.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
