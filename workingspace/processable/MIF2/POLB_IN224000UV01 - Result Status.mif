<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.5" name="224000"
                 title="Result Event Status">
   <mif:businessName name="Result Status"/>
   <mif:packageLocation root="DEFN" section="HM" subSection="PO" domain="LB" artifact="IN"
                        realmNamespace="UV"
                        version="01"
                        id="224000"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text> This interaction is a Result Status. This interaction is used in those situations where Laboratory system sends a status message indicating where the lab is in the process of completing the result. This interaction should not be used to communicate actual results. This includes process steps such as:<mif:li>
                  <mif:p>Specimen collected</mif:p>
               </mif:li>
               <mif:li>
                  <mif:p>Specimen received in lab</mif:p>
               </mif:li>
               <mif:li>
                  <mif:p>Specimen accessioned</mif:p>
               </mif:li>
               <mif:li>
                  <mif:p>Testing begun</mif:p>
               </mif:li> And other process steps that maybe appropriate. There is no state change and the report continues to be pending. </mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="HM" subSection="PO" domain="LB" artifact="TE"
                             realmNamespace="UV"
                             version="01"
                             id="004000"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT"
                        realmNamespace="UV"
                        version="01"
                        id="000100">
      <mif:parameterModel parameterName="ControlActProcess" root="DEFN" section="IM" subSection="MC"
                          domain="AI"
                          artifact="MT"
                          realmNamespace="UV"
                          version="01"
                          id="700201"
                          traversalName="controlActProcess">
         <mif:parameterModel parameterName="Act" root="DEFN" section="HM" subSection="PO" domain="LB"
                             artifact="MT"
                             realmNamespace="UV"
                             version="01"
                             id="004000"
                             traversalName="observationEventChoice">
            <mif:specialization className="ObservationReport" traversalName="observationReport"/>
            <mif:specialization className="ObservationBattery" traversalName="observationBattery"/>
            <mif:specialization className="SpecimenObservationCluster"
                                traversalName="specimenObservationCluster"/>
            <mif:specialization className="ObservationEvent" traversalName="observationEvent"/>
         </mif:parameterModel>
      </mif:parameterModel>
   </mif:argumentMessage>
</mif:interaction>