<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.5" name="201304"
                 title="Patient Livingsubject Event Obsolete Notification">
   <mif:businessName name="Patient Registry Duplicates Resolved"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="PR" domain="PA" artifact="IN"
                        realmNamespace="UV"
                        version="02"
                        id="201304"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>A patient registry sends this <mif:b>notification</mif:b> after <mif:b>resolving duplicate registrations</mif:b> in the registry. The surviving registration (<mif:i>RegistrationEvent.statusCode</mif:i> = "active") links via the <mif:i>replacementOf </mif:i>act relationship to the deprecated registration (<mif:i>PriorRegistration.statusCode</mif:i> = "obsolete"). A copy of the surviving person record is sent in the payload message.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="AM" subSection="PR" domain="PA" artifact="TE"
                             realmNamespace="UV"
                             version="02"
                             id="201304"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT"
                        realmNamespace="UV"
                        version="01"
                        id="000100">
      <mif:parameterModel parameterName="ControlActProcess" root="DEFN" section="IM" subSection="MF"
                          domain="MI"
                          artifact="MT"
                          realmNamespace="UV"
                          version="01"
                          id="700701"
                          traversalName="controlActProcess">
         <mif:parameterModel parameterName="RegisteredRole" root="DEFN" section="AM" subSection="PR"
                             domain="PA"
                             artifact="MT"
                             realmNamespace="UV"
                             version="02"
                             id="201303"
                             traversalName="patient"/>
      </mif:parameterModel>
   </mif:argumentMessage>
</mif:interaction>