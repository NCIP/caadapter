         <mif:attribute name="reasonCode" sortKey="11" minimumMultiplicity="0" maximumMultiplicity="*"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The Observationevent.reason attribute is a code specifying the motivation, cause, or rationale of the observation when such rationale is not reasonably representable as an ActRelationship of type "reason" linking to A_SupportingClinicalStatement universal act. This reason applies to the entire ObservationEvent rather than to one or more sub-components.</html:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
               <mif:appInfo>
                  <mif:mapping sourceName="Unspecified">
