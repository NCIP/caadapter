            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="id" sortKey="3" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The ActDefinition.id attribute is used to distinguish this definition from any other definition.</html:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
               <mif:appInfo>
                  <mif:mapping sourceName="Unspecified">
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">The closest 2.x analogs for this attribute is Universal Servce ID OBR 4 or Observation Identifier OBX 3 which are communicated as codes but in V3 will be communicated via id with the II data type.</html:p>
                     </mif:text>
                  </mif:mapping>
