                 title="Authorization Event Complete Notification Initial/Complete Vision Care">
   <mif:businessName name="Auth Comp Results Init/Comp, VisionCare"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="FI" domain="CR" artifact="IN"
                        realmNamespace="UV"
                        version="03"
                        id="310306"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>The sender sends an Authorization Results message containing the completed results of an Authorization Request for Vision Care services and/or products. This notification follows on initial results sent previously. Authorization Results indicate approval or disapproval to render the requested services and/or products, as well as the extent to which the Adjudicator will pay for those services and/or products.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
