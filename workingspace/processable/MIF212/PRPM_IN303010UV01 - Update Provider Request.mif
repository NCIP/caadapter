<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.2" name="303010"
                 title="Provider Event Revise Request">
   <mif:businessName name="Update Provider Request"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="PR" domain="PM" artifact="IN"
                        realmNamespace="UV"
                        version="01"
                        id="303010"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>This interaction is used to request that a provider record be updated within a provider registry system. It is expected that the receiving application will respond with an Update Provider Response (PRPM_IN303011) interaction containing the appropriate identifiers assigned by the provider registry system.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="AM" subSection="PR" domain="PM" artifact="TE"
                             realmNamespace="UV"
                             version="01"
                             id="303010"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT"
                        realmNamespace="UV"
                        version="01"
                        id="000100">
      <mif:parameterModel parameterName="ControlActProcess" root="DEFN" section="IM" subSection="MF"
                          domain="MI"
                          artifact="MT"
                          realmNamespace="UV"
                          version="01"
                          id="700721"
                          traversalName="controlActProcess">
         <mif:parameterModel parameterName="RegisteredRole" root="DEFN" section="AM" subSection="PR"
                             domain="PM"
                             artifact="MT"
                             realmNamespace="UV"
                             version="01"
                             id="303010"
                             traversalName="roleChoice">
            <mif:specialization className="AssignedEntity" traversalName="assignedEntity"/>
            <mif:specialization className="HealthCareProvider" traversalName="healthCareProvider"/>
            <mif:specialization className="QualifiedEntity" traversalName="qualifiedEntity"/>
         </mif:parameterModel>
      </mif:parameterModel>
   </mif:argumentMessage>
   <mif:receiverResponsibilities>
      <mif:reason>
         <html:text xmlns:html="http://www.w3.org/1999/xhtml">  
                  <html:p>Registry response including key identifiers</html:p>  
               </html:text>
      </mif:reason>
      <mif:invokeInteraction root="DEFN" section="AM" subSection="PR" domain="PM" artifact="IN"
                             realmNamespace="UV"
                             version="01"
                             id="303011"/>
      <mif:invokeTriggerEvent root="DEFN" section="AM" subSection="PR" domain="PM" artifact="TE"
                              realmNamespace="UV"
                              version="01"
                              id="303011"/>
   </mif:receiverResponsibilities>
</mif:interaction>