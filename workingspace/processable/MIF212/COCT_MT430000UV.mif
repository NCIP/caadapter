   <mif:entryPoint graphicLinkId="2882E4B3-B98A-3CA9-F33E-3E6F343E6B37" id="COCT_MT430000UV"
                   name="R_LabTestKit universal"
                   className="LabTestKit">
      <mif:annotations>
         <mif:documentation>
            <mif:description>
               <mif:text>Description: Used to identify a test kit used for laboratory testing.</mif:text>
            </mif:description>
         </mif:documentation>
      </mif:annotations>
   </mif:entryPoint>
