   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="9FF8893B-5D6E-CB02-85B4-63DCCF1929B7" name="LabTestKit"
                 isAbstract="false">
         <mif:derivedFrom staticModelDerivationId="1" className="Role"/>
         <mif:derivedFrom staticModelDerivationId="2" className="LabTestKit"/>
         <mif:derivedFrom staticModelDerivationId="3" className="LabTestKit"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">The lab test kit is a manufactured product role played by a manufactured material entity (TestKit). The role is scoped by the organization (E_Organization) manufacturing the test kit.The scoping organization is mandatory when the manufactured material entity (TestKit) lotNumberText attribute is populated.</html:p>
                  </mif:text>
               </mif:definition>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="classCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="MANU"
                        isImmutable="true">
            <mif:derivedFrom staticModelDerivationId="1" className="Role" attributeName="classCode"/>
            <mif:derivedFrom staticModelDerivationId="2" className="LabTestKit" attributeName="classCode"/>
            <mif:derivedFrom staticModelDerivationId="3" className="LabTestKit" attributeName="classCode"/>
            <mif:type name="CS"/>
            <mif:vocabulary codingStrength="CNE">
               <mif:code codeSystemName="RoleClass" code="MANU"/>
            </mif:vocabulary>
         </mif:attribute>
         <mif:attribute name="id" sortKey="2" minimumMultiplicity="0" maximumMultiplicity="*"
                        isMandatory="false"
                        isImmutable="false">
            <mif:annotations>
               <mif:documentation>
                  <mif:definition>
                     <mif:text>
                        <html:p xmlns:html="http://www.w3.org/1999/xhtml">A unique identifier for the a specific instance of a lab test kit. The id is scoped by the test kit manufacturer. This manufactured product role id is not the lot number associated with the test kit. Generally, lot numbers are not globally unique, rather they require knowledge of both the manufacturer and the type of test kit.</html:p>
                     </mif:text>
                  </mif:definition>
               </mif:documentation>
            </mif:annotations>
            <mif:derivedFrom staticModelDerivationId="1" className="Role" attributeName="id"/>
            <mif:derivedFrom staticModelDerivationId="2" className="LabTestKit" attributeName="id"/>
            <mif:derivedFrom staticModelDerivationId="3" className="LabTestKit" attributeName="id"/>
            <mif:type name="SET">
               <mif:argumentDatatype name="II"/>
            </mif:type>
         </mif:attribute>
      </mif:class>
   </mif:containedClass>
   <mif:containedClass>
      <mif:class graphicLinkId="22C369FB-CEA5-8701-3B8A-EFBFDA18CAA7" name="TestKit"
                 isAbstract="false">
