   <mif:containedClass>
      <mif:class graphicLinkId="93D646A2-98E3-9955-E967-1E590B970188" name="Subject2"
                 isAbstract="false">
         <mif:businessName name="Adverse Event Indicator"/>
         <mif:derivedFrom staticModelDerivationId="1" className="ActRelationship"/>
         <mif:derivedFrom staticModelDerivationId="2" className="Subject2"/>
         <mif:annotations>
            <mif:documentation>
               <mif:definition>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Indicates whether there was an adverse event associated with the immunization.</html:p>
                  </mif:text>
               </mif:definition>
               <mif:rationale>
                  <mif:text>
                     <html:p xmlns:html="http://www.w3.org/1999/xhtml">Tracking adverse events is an important part of reporting immunizations. The presence or absence of this class behaves as a 'mandatory' indicator where the presence indicates an adverse reaction and the absence indicates no known information.</html:p>
                  </mif:text>
               </mif:rationale>
            </mif:documentation>
         </mif:annotations>
         <mif:attribute name="typeCode" sortKey="1" minimumMultiplicity="1" maximumMultiplicity="1"
                        conformance="R"
                        isMandatory="true"
                        fixedValue="SUBJ"
                        isImmutable="true">
