<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.2" name="020001"
                 title="AnnotatedECG Revised, Notification">
   <mif:businessName name="AnnotatedECG Revised, Notification"/>
   <mif:packageLocation root="DEFN" section="HM" subSection="PO" domain="RT" artifact="IN"
                        realmNamespace="UV"
                        version="01"
                        id="020001"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>This interaction is used by the ECG analysis system to transmit new or revised ECG annotations to the sponsor of a clinical trial at a scheduled time or in response to an external event.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="HM" subSection="PO" domain="RT" artifact="TE"
                             realmNamespace="UV"
                             version="01"
                             id="010001"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT"
                        realmNamespace="UV"
                        version="01"
                        id="000100">
      <mif:parameterModel parameterName="ControlActProcess" root="DEFN" section="IM" subSection="MC"
                          domain="AI"
                          artifact="MT"
                          realmNamespace="UV"
                          version="01"
                          id="700201"
                          traversalName="controlActProcess">
         <mif:parameterModel parameterName="Act" root="DEFN" section="HM" subSection="PO" domain="RT"
                             artifact="MT"
                             realmNamespace="UV"
                             version="01"
                             id="020001"
                             traversalName="annotatedECG"/>
      </mif:parameterModel>
   </mif:argumentMessage>
   <mif:receiverResponsibilities>
      <mif:reason>
         <html:text xmlns:html="http://www.w3.org/1999/xhtml">  
                  <html:p>If instance of focal class exists, replace all existing data for that instance with the data in this message; If instance of focal class does not exist, add the data in this message.</html:p>  
               </html:text>
      </mif:reason>
      <mif:invokeInteraction root="DEFN" section="HM" subSection="PO" domain="RT" artifact="IN"
                             realmNamespace="UV"
                             version="01"
                             id="020001"/>
      <mif:invokeTriggerEvent root="DEFN" section="HM" subSection="PO" domain="RT" artifact="TE"
                              realmNamespace="UV"
                              version="01"
                              id="010001"/>
   </mif:receiverResponsibilities>
</mif:interaction>