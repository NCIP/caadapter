<?xml version="1.0" encoding="UTF-8"?>
<mif:interaction xmlns:mif="urn:hl7-org:v3/mif2" schemaVersion="2.1.2" name="202304"
                 title="Service Delivery Location Event Obsolete Notification">
   <mif:businessName name="Location Registry Duplicates Resolved"/>
   <mif:packageLocation root="DEFN" section="AM" subSection="PR" domain="PA" artifact="IN"
                        realmNamespace="UV"
                        version="02"
                        id="202304"/>
   <mif:annotations>
      <mif:documentation>
         <mif:description>
            <mif:text>A service delivery location registry sends this <html:b xmlns:html="http://www.w3.org/1999/xhtml">notification</html:b> after <html:b xmlns:html="http://www.w3.org/1999/xhtml">resolving duplicate registrations</html:b> in the registry. The surviving registration (<html:i xmlns:html="http://www.w3.org/1999/xhtml">RegistrationEvent.statusCode</html:i> = "active") links via the <html:i xmlns:html="http://www.w3.org/1999/xhtml">replacementOf </html:i>act relationship to the deprecated registration (<html:i xmlns:html="http://www.w3.org/1999/xhtml">PriorRegistration.statusCode</html:i> = "obsolete"). A copy of the surviving location record is sent in the payload message.</mif:text>
         </mif:description>
      </mif:documentation>
   </mif:annotations>
   <mif:invokingTriggerEvent root="DEFN" section="AM" subSection="PR" domain="PA" artifact="TE"
                             realmNamespace="UV"
                             version="02"
                             id="202304"/>
   <mif:argumentMessage root="DEFN" section="IM" subSection="MC" domain="CI" artifact="MT"
                        realmNamespace="UV"
                        version="01"
                        id="000100">
      <mif:parameterModel parameterName="ControlActProcess" root="DEFN" section="IM" subSection="MF"
                          domain="MI"
                          artifact="MT"
                          realmNamespace="UV"
                          version="01"
                          id="700701"
                          traversalName="controlActProcess">
         <mif:parameterModel parameterName="RegisteredRole" root="DEFN" section="AM" subSection="PR"
                             domain="PA"
                             artifact="MT"
                             realmNamespace="UV"
                             version="02"
                             id="202303"
                             traversalName="serviceDeliveryLocation"/>
      </mif:parameterModel>
   </mif:argumentMessage>
</mif:interaction>