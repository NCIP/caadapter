	<formula type="math" name="BSA6">
		<annotation>Boyd formula</annotation>
		<expression type="expression" name="BSA6" description="(m^2)" operation="multiplication">
			<term type="constant" name="factor" value="0.0003207" />
			<term type="expression" name="factor" operation="multiplication">
				<term type="expression" name="factor" operation="power">
					<term type="variable" name="base" value="weight" description="(kg)" /> 
					<term type="expression" name="index" operation="subtraction" >
						<term type="constant" name="minuend" value="0.7285" />  
						<term type="expression" name="subtrahend" operation="multiplication">
							<term type="constant" name="factor" value="0.0188"/> 						
							<term type="expression" name="factor" operation="logarithm">
								<term type="constant" name="base" value="10"/> 
								<term type="variable" name="index" value="weight" description="(kg)" /> 
							</term> 
						</term>	
					</term>
				</term>
				<term type="expression" name="factor" operation="power">
					<term type="variable" name="base" value="height" description="(cm)" />
					<term type="constant" name="index" value="0.3" />
				</term> 
			</term> 
		</expression>
	</formula>
