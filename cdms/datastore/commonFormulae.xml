<?xml version="1.0" encoding="UTF-8"?>
<formulaStore name="Common Formula">
	<formula type="math" name="BSA1">
		<annotation>Dubois  Dubois formula</annotation>
		<expression type="expression" name="BSA1" description="(m^2)" operation="multiplication">
			<term type="constant" name="factor" value="0.00718" />
			<term type="expression" name="factor" operation="multiplication">
				<term type="expression" name="factor" operation="power">
					<term type="variable" name="base" description="(kg)" value="weight"/> 
					<term type="constant" name="index" value="0.425"/>
				</term>
				<term type="expression" name="factor" operation="power">
					<term type="variable" name="base" description="(cm)" value="height"/>
					<term type="constant" name="index" value="0.725"/>
				</term>
			</term>
		</expression>
	</formula>
	<formula type="math" name="BSA2">
		<annotation>Dubois  Dubois formula: Alternative</annotation>
		<expression type="expression" name="BSA2" description="(m^2)" operation="division">
			<term type="expression" name="dividend" operation="multiplication">
				<term type="expression" name="factor" operation="power">
					<term type="variable" name="base" description="(kg)" value="weight"/> 
					<term type="constant" name="index" value="0.425"/>
				</term>
				<term type="expression" name="factor" operation="power">
					<term type="variable" name="base" description="(cm)" value="height"/>
					<term type="constant" name="index" value="0.725"/>
				</term>
			</term>
			<term type="constant" name="divisor" value="139.2" />
		</expression>
	</formula>
	<formula type="math" name="BSA3">
		<annotation>Mosteller formula: used by Pharmacy and Therapeutics Committee of Cross Cancer Institue, Edmonton, Alberta, Canada</annotation>
		<expression type="expression" name="BSA3" description="(m^2)" operation="squareRoot">
			<term type="expression" name="base" operation="division">
				<term type="expression" name="dividend" operation="multiplication">
						<term type="variable" name="factor" description="(kg)" value="weight"/> 
						<term type="variable" name="factor" description="(cm)" value="height"/>
				</term>
				<term type="constant" name="divisor" value="3600" />
			</term>
		</expression>
	</formula>
